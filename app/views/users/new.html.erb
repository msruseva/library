<%= render partial: 'error' %>

<h2 style="color: #AB6BE0;">Create User</h2>

<%= form_for(@user) do |f| %>
	<%= f.label :username %> <%= f.text_field :username %>
	<%= f.label :password %> <%= f.password_field :password %>
	<%= f.submit %>
<% end %>

<%= button_to "Already have account", login_path, method: :get %>